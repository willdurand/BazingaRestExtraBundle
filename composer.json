{
    "name": "willdurand/rest-extra-bundle",
    "description": "This bundle provides extra features for your REST APIs built using Symfony2.",
    "keywords": [ "rest", "api" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
        "name": "William Durand",
        "email": "will+git@drnd.me"
    }
    ],
    "require": {
        "symfony/framework-bundle": "^3.4|^4.0"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "^4.0|^5.0",
        "symfony/browser-kit": "^3.4|^4.0",
        "symfony/finder": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/yaml": "^3.4|^4.0"
    },
    "autoload": {
        "psr-4": { "Bazinga\\Bundle\\RestExtraBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
